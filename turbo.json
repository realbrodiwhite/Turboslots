  {
    "$schema": "https://turborepo.org/schema.json",
    "pipeline": {
      "cache": {
        "type": "filesystem",
        "location": "node_modules/.cache/turborepo",
        "folder": ".turbo",
        "outputs": ["**/*.cache.json"],
        "caching": [
          "node_modules",
          "node_modules/**",
          "node_modules/**/*"]
      },
      "start": {
        "dependsOn": ["start"],
        "output": ["start"],
        "command": ["node index.js"]
      },
      "run": {
        "dependsOn": ["^run"],
        "outputs": ["run/**"],
        "commands": [
          "npm run build:client",
          "npm run start",
          "node index.js"]
      },
      "run:client": {
        "dependsOn": ["^run"],
        "outputs": ["run/**"],
        "commands": [
          "cd casino-monorepo/client",
          "npm run build"]
      },
      "run:server":  {
        "dependsOn": ["^run"],
        "outputs": ["run/**"],
        "commands": ["cd casino-monorepo/server", "npm run start"]
      },
      "build:client": {
          "dependsOn": ["^build"],
          "outputs": ["dist/**", "build/**", "public/**"],
          "command": ["cd casino-monorepo/client && npm run build"]
        },
        "build:server": {
          "dependsOn": ["^build"],
          "outputs": ["server/build/**", "server/public/**"]
        },
        "test": {
          "dependsOn": ["^build"],
          "outputs": []
        },
        "lint": {
          "outputs": []
        }
      }
    }